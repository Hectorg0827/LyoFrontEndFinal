{
  "name": "lyo-app",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "postinstall": "patch-package",
    "patch:podfile": "node scripts/patch-podfile.js",
    "patch:pods": "node scripts/patch-pods.js",
    "patch:expo-asset": "node scripts/update-expo-asset-patch.js",
    "update:deps": "node scripts/update-dependencies.js",
    "fresh:ios": "node scripts/fresh-ios-build.js",
    "clean:ios": "rm -rf ios/build ios/Pods ios/Podfile.lock ~/Library/Developer/Xcode/DerivedData",
    "clean:all": "npm run clean:ios && rm -rf node_modules package-lock.json",
    "setup:ios": "npm install && npx pod-install",
    "prebuild:ios": "npx expo prebuild --platform ios --clean",
    "build:ios": "npm run prebuild:ios && npm run patch:podfile && npm run patch:pods && cd ios && pod install && cd .. && npx expo run:ios",
    "fix:ios": "npm run clean:ios && npm run build:ios",
    "doctor": "npx expo-doctor",
    "check:project": "node scripts/check-project.js",
    "reset:ios": "rm -rf ios && npm run clean:all && npm install && npm run prebuild:ios && npm run patch:podfile && npm run patch:pods && npm run check:project && cd ios && pod install",
    "reset:ios:proper": "node scripts/reset-ios-properly.js",
    "fix:red-files": "node scripts/fix-red-files.js",
    "fix:xcode-project": "node scripts/fix-xcode-project.js",
    "check:enhanced": "node scripts/enhanced-project-check.js",
    "fix:permissions": "node scripts/fix-file-permissions.js"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^4",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/datetimepicker": "8.0.1",
    "@react-native-community/slider": "4.5.2",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.11.0",
    "@shopify/flash-list": "1.6.4",
    "@tanstack/react-query": "^5.59.20",
    "@types/react-native-fs": "^2.13.0",
    "expo": "~51.0.14",
    "expo-application": "~5.9.1",
    "expo-asset": "~10.0.6",
    "expo-av": "~14.0.7",
    "expo-blur": "~13.0.2",
    "expo-dev-client": "~4.0.27",
    "expo-device": "~6.0.2",
    "expo-file-system": "~17.0.1",
    "expo-image": "~1.13.0",
    "expo-linear-gradient": "~13.0.2",
    "expo-linking": "~6.3.1",
    "expo-localization": "~15.0.3",
    "expo-network": "~6.0.1",
    "expo-notifications": "~0.28.19",
    "expo-speech": "~12.0.2",
    "expo-splash-screen": "~0.27.6",
    "expo-status-bar": "~1.12.1",
    "expo-updates": "~0.25.25",
    "i18n-js": "^4.4.3",
    "nativewind": "^2.0.11",
    "react": "18.2.0",
    "react-native": "0.74.5",
    "react-native-dotenv": "^3.4.11",
    "react-native-fs": "^2.20.0",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-maps": "1.14.0",
    "react-native-markdown-display": "^7.0.2",
    "react-native-onboarding-swiper": "^1.3.0",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "sentry-expo": "~7.0.0",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@eslint/eslintrc": "^3.3.1",
    "@types/node": "^22.15.20",
    "@types/react": "~18.2.79",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "eslint": "^9.27.0",
    "eslint-config-universe": "^15.0.3",
    "globals": "^16.1.0",
    "jest": "^29.7.0",
    "patch-package": "^8.0.0",
    "tailwindcss": "^3.4.14",
    "typescript": "~5.3.3",
    "typescript-eslint": "^8.32.1"
  },
  "private": true
}
